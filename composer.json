{
    "name": "frontaccounting/mrp-module",
    "description": "Material Requirements Planning module for FrontAccounting",
    "type": "library",
    "version": "1.0.0",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "FrontAccounting Team",
            "email": "info@frontaccounting.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "psr/log": "^3.0",
        "psr/event-dispatcher": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "FA\\Modules\\MRP\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FA\\Modules\\MRP\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}